<settings>
	<proxies>
		<proxy>
			<id>proxy-http</id>
			<active>true</active>
			<protocol>http</protocol>
			<host>172.18.0.1</host>
			<port>3128</port>
			<nonProxyHosts>127.0.0.*|172.*.*.*|localhost</nonProxyHosts>
		</proxy>
		<proxy>
			<id>proxy-https</id>
			<active>true</active>
			<protocol>https</protocol>
			<host>172.18.0.1</host>
			<port>3128</port>
			<nonProxyHosts>127.0.0.*|172.*.*.*|localhost</nonProxyHosts>
		</proxy>
	</proxies>
	<mirrors>
		<mirror>
			<id>maven-default-http-blocker</id>
			<mirrorOf>dummy</mirrorOf>
			<name>Dummy mirror to override default blocking mirror that blocks http</name>
			<url>http://0.0.0.0/</url>
		</mirror>
	</mirrors>
</settings>